#include <bits/stdc++.h>
using namespace std;

int main(){
    std::ios::sync_with_stdio(false);    
    cin.tie(NULL);
    int t, n;
    cin>>t;
    while(t--){
        int i=3; 
        cin>>n;
        while(n--){
            cout<<i<<" ";
            i += 2;
        }
        cout<<"\n";
    }
    return 0;
}
