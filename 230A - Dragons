#include <bits/stdc++.h>
using namespace std;

int main(){
    std::ios::sync_with_stdio(false);    
    cin.tie(NULL);
    int s,n;
    cin>>s>>n;
    vector<pair<int, int>> v(n);
    for(int i=0; i<n; ++i){
        cin>>v[i].first>>v[i].second;
    }
    sort(v.begin(), v.end());
    int i=0;
    for(i=0; i<n; ++i){
        if(s<=v[i].first)   break;
        else    s += v[i].second;
    }
    (i==n)?cout<<"YES":cout<<"NO";
    
    return 0;
}
