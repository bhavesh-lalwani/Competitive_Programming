#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int t,n;
    cin>>t;
    while(t--){
        cin>>n;
        int temp,odd=0;
        for(int i=0;i<2*n; ++i){
            cin>>temp;
            if(temp&1)    odd++;
        }
        (odd==n)?cout<<"YES\n":cout<<"NO\n";
    }
    return 0;
}
