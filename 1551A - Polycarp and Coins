#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int t,n,b1,b2;
    cin>>t;
    while(t--){
        cin>>n;
        b1=n/3,b2=n/3;
        if(b1 + 2*b2 != n){
            if(n-(b1 + 2*b2) ==1)   b1++;
            else    b2++;
        }
        cout<<b1<<" "<<b2<<"\n";
    }
    return 0;
}
