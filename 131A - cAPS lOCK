#include <bits/stdc++.h>
using namespace std;

int main(){
    std::ios::sync_with_stdio(false);    
    cin.tie(NULL);
    string s;
    cin>>s;
    if(s.length()==1 ){
        if(s[0]>='a' && s[0]<='z')  cout<<char(s[0]-32);
        else    cout<<char(s[0]+32);
    }
    else{
        bool no_change=false;
        for(int i=1; i<s.length(); ++i){
            if(s[i]>='a' && s[i]<='z'){
                no_change=true;
                break;
            }
        }
        if(!no_change){
            if(s[0]>='a' && s[0]<='z')  s[0] -= 32;
            else    s[0] += 32;
            for(int i=1; i<s.length(); ++i) s[i] += 32;
        }
        cout<<s;
    }
}
