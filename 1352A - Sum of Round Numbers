#include <bits/stdc++.h>
using namespace std;
int main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    string n;
    cin>>t;
    while(t--){
        cin>>n;
        vector<int> v;
        for(int i=n.length()-1; i>=0; --i){
            if(n[i]-'0'!=0){
                v.emplace_back((n[i]-'0')*pow(10, n.length()-1-i));
            }
        }
        reverse(v.begin(),v.end());
        cout<<v.size()<<"\n";
        for(int i=0; i<v.size(); ++i){
            cout<<v[i]<<" ";
        }
        cout<<"\n";
    }
    return 0;
}
